@model IEnumerable<WebApplication.Models.ClienteModel>

<table class="table table-striped table-bordered" id="tbListarClientes">
    <thead>
        <tr id="cabecalho">
            <th>
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CNPJ_CPF)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RazaoSocial_Nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Telefone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CEP)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Logradouro)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Logradouro_Cidade)/@Html.DisplayNameFor(model => model.Logradouro_UF)
            </th>
            <th>
                @*@Html.DisplayNameFor(model => model.CEP)*@
                Chamado Aberto
            </th>
            <th>
                Detalhes
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <input type="checkbox" id="cbCliente" name="cbClilente" />
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CNPJ_CPF)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.RazaoSocial_Nome)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Telefone)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CEP)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Logradouro)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Logradouro_Cidade)/@Html.DisplayFor(modelItem => item.Logradouro_UF)
                </td>
                <td>
                    @*@Html.DisplayFor(modelItem => item.Pais)*@
                    Sim
                </td>
                <td>
                    <a href="javascript:;" onclick="Cliente.Visualizar(@item.Id)" title="Visualizar"><i class="fa fa-plus-circle"></i></a>
                    <a href="javascript:;" onclick="Cliente.Visualizar(@item.Id)" title="Visualizar"><i class="fa fa-search"></i></a>
                    <a href="javascript:;" onclick="Cliente.Editar(@item.Id)" title="Editar"><i class="fa fa-edit"></i></a>
                    <a href="javascript:;" onclick="Cliente.Excluir(@item.Id)" title="Excluir"><i class="fa fa-trash"></i></a>
                </td>
            </tr>
        }
    </tbody>
</table>
